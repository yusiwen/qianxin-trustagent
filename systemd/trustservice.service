[Unit]
Description=trustservice Service
Documentation=
Wants=network.target
After=network.target

[Service]
Type=simple
WorkingDirectory=/opt/apps/com.qianxin.trustagent/files/bin/
ExecStart=/opt/apps/com.qianxin.trustagent/files/bin/trustservice --nofork
User=root
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
